        Title="Přidat model" Height="235" Width="400"
        xmlns:uc="clr-namespace:CarSell.UserControlComponent"
        WindowStartupLocation="CenterScreen">
    <Grid Margin="10,10,10,0" Height="164" VerticalAlignment="Top">

        <StackPanel>
            <uc:ModelForm x:Name="ModelFormControl" />

            <Button Content="Přidat"  HorizontalAlignment="Center" Width="100"
